{
	"expo": {
		"name": "Boxly2.0Mobile",
		"slug": "Boxly2.0Mobile",
		"scheme": "boxlyapp",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/icon/splashscreen.png",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icon/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.myapp.io",
              "pathPrefix": "/records"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
		},
		"web": {
			"favicon": "./assets/icon/favicon.png"
		},
		"plugins": ["expo-router", "expo-secure-store", "expo-font"]
	}
}
